###  http://purl.jp/bio/01/quanto/sos#encoding
:encoding rdf:type owl:DatatypeProperty ;
          rdfs:domain :SequenceStatisticsReport ;
          rdfs:range xsd:string ;
          dc:description "A relation between a SequenceStatisticsReport and a string value representing quality encoding format of fastq data."@en ;
          rdfs:label "encoding" .


###  http://purl.jp/bio/01/quanto/sos#fastqcVersion
:fastqcVersion rdf:type owl:DatatypeProperty ;
               rdfs:domain :SequenceStatisticsReport ;
               rdfs:range xsd:string ;
               dc:description "A relation between a SequenceStatisticsReport and a value representing the version of FastQC, the software to calculate statistics of sequence data."@en ;
               rdfs:label "fastqcVersion"@en .


###  http://purl.jp/bio/01/quanto/sos#fileType
:fileType rdf:type owl:DatatypeProperty ;
          rdfs:domain :SequenceStatisticsReport ;
          rdfs:range xsd:string ;
          dc:description "A relation between a SequenceStatisticsReport and a file type of the calculated fastq file that represents whether its content was base call or colorspace."@en ;
          rdfs:label "fileType"@en .


###  http://purl.jp/bio/01/quanto/sos#filename
:filename rdf:type owl:DatatypeProperty ;
          rdfs:domain :SequenceStatisticsReport ;
          rdfs:range xsd:string ;
          dc:description "A relation between a SequenceStatisticsReport and a name of the file that contained the sequencing data calculated statistics."@en ;
          rdfs:label "filename"@en .


#################################################################
