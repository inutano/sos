###  http://purl.jp/bio/01/quanto/sos#BaseRatio
:BaseRatio rdf:type owl:Class ;
           rdfs:subClassOf :SequenceQuality ;
           dc:description "A BaseRatio is a sequence quality that describes the statistics represented by ratio in nucleotide base resolution."@en ;
           rdfs:label "BaseRatio"@en .


###  http://purl.jp/bio/01/quanto/sos#PhredQualityScore
:PhredQualityScore rdf:type owl:Class ;
                   rdfs:subClassOf :SequenceQuality ;
                   dc:description "A PhredQualityScore is a sequence quality that represents accuracy of base call of DNA sequencing data in Phred quality score."@en ;
                   rdfs:label "PhredQualityScore"@en .


###  http://purl.jp/bio/01/quanto/sos#SequenceQuality
:SequenceQuality rdf:type owl:Class ;
                 dc:description "A SequenceQuality is an attribute that is associated with a quantitative value calculated from high-throughput sequencing data."@en ;
                 rdfs:label "SequenceQuality"@en .


###  http://purl.jp/bio/01/quanto/sos#SequenceReadAmount
:SequenceReadAmount rdf:type owl:Class ;
                    rdfs:subClassOf :SequenceQuality ;
                    dc:description "A SequenceReadAmount is a sequence quality that represents number of sequence reads."@en ;
                    rdfs:label "SequenceReadAmount"@en .


###  http://purl.jp/bio/01/quanto/sos#SequenceReadLength
:SequenceReadLength rdf:type owl:Class ;
                    rdfs:subClassOf :SequenceQuality ;
                    dc:description "A SequenceReadLength is a sequence quality that represents length of sequence read."@en ;
                    rdfs:label "SequenceReadLength"@en .


###  http://purl.jp/bio/01/quanto/sos#SequenceStatisticsReport
:SequenceStatisticsReport rdf:type owl:Class ;
                          rdfs:subClassOf owl:Thing ;
                          dc:description "A SequenceStatisticsReport is an entity that contains the summary of quantitative information of sequencing data."@en ;
                          rdfs:label "SequenceStatisticsReport" .


###  http://purl.jp/bio/01/quanto/sos#filteredSequences
:filteredSequences rdf:type owl:Class ;
                   rdfs:subClassOf :SequenceReadAmount ;
                   dc:description "A filteredSequences is a sequence quality that represents the amount of sequence reads filtered during the sequence statistics calculation."@en ;
                   rdfs:label "filteredSequences"@en .


###  http://purl.jp/bio/01/quanto/sos#maxSequenceLength
:maxSequenceLength rdf:type owl:Class ;
                   rdfs:subClassOf :SequenceReadLength ;
                   dc:description "A maxSequenceLength is a sequence quality that represents the maximum length of sequence reads in a sequencing data."@en ;
                   rdfs:label "maxSequenceLength"@en .


###  http://purl.jp/bio/01/quanto/sos#meanBaseCallQuality
:meanBaseCallQuality rdf:type owl:Class ;
                     rdfs:subClassOf :PhredQualityScore ;
                     dc:description "A meanBaseCallQuality is a sequence quality that represents the mean value of phred quality score in the all base calls of a sequence data."@en ;
                     rdfs:label "meanBaseCallQuality"@en .


###  http://purl.jp/bio/01/quanto/sos#meanSequenceLength
:meanSequenceLength rdf:type owl:Class ;
                    rdfs:subClassOf :SequenceReadLength ;
                    dc:description "A meanSequenceLength is a sequence quality that represents the mean length of sequence reads in a sequencing data."@en ;
                    rdfs:label "meanSequenceLength"@en .


###  http://purl.jp/bio/01/quanto/sos#medianBaseCallQuality
:medianBaseCallQuality rdf:type owl:Class ;
                       rdfs:subClassOf :PhredQualityScore ;
                       dc:description "A medianBaseCallQuality is a sequence quality that represents the median value of phred quality score in the all base calls of a sequence data."@en ;
                       rdfs:label "medianBaseCallQuality"@en .


###  http://purl.jp/bio/01/quanto/sos#medianSequenceLength
:medianSequenceLength rdf:type owl:Class ;
                      rdfs:subClassOf :SequenceReadLength ;
                      dc:description "A medianSequenceLength is a sequence quality that represents the median length of sequence reads in a sequencing data."@en ;
                      rdfs:label "medianSequenceLength"@en .


###  http://purl.jp/bio/01/quanto/sos#minimumSequenceLength
:minimumSequenceLength rdf:type owl:Class ;
                       rdfs:subClassOf :SequenceReadLength ;
                       dc:description "A minimumSequenceLength is a sequence quality that represents the minimum length of sequence reads in a sequencing data."@en ;
                       rdfs:label "minimumSequenceLength"@en .


###  http://purl.jp/bio/01/quanto/sos#nContent
:nContent rdf:type owl:Class ;
          rdfs:subClassOf :BaseRatio ;
          dc:description "A nContent is a sequence quality that represents ratio of N, which means a failure of base calling of DNA sequencing in fastq format, in the all sequenced bases."@en ;
          rdfs:label "nContent"@en .


###  http://purl.jp/bio/01/quanto/sos#percentGC
:percentGC rdf:type owl:Class ;
           rdfs:subClassOf :BaseRatio ;
           dc:description "A percentGC is a sequence quality that represents a ratio of G and C, which means Guanine and Cytosine in fastq format, respectively, in the all sequenced bases."@en ;
           rdfs:label "percentGC"@en .


###  http://purl.jp/bio/01/quanto/sos#totalSequences
:totalSequences rdf:type owl:Class ;
                rdfs:subClassOf :SequenceReadAmount ;
                dc:description "A totalSequences is a sequence quality that represents the total amount of sequence reads in a calculated sequencing data."@en ;
                rdfs:label "totalSequences"@en .


